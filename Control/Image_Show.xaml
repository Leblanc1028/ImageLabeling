             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Label_Monster.Control"
             mc:Ignorable="d" 
             >
    <Grid x:Name="Gr0" MouseLeftButtonUp="On_Left_Mouse_Up" MouseRightButtonUp="On_Right_Mouse_Up">
        <Viewbox x:Name="V0" HorizontalAlignment="Left" Height="763" Width="911"  VerticalAlignment="Top" >
            <Image x:Name="Label_Image" Height="763" Width="911" />
        </Viewbox>
    </Grid>
</UserControl>
